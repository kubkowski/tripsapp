<h1>Itineraries</h1>

<table>
  <tr>
    <th>Location</th>
    <th>Travel Date</th>
  </tr>
<% @itineraries.each do |itinerary| %>
  <tr>
    <td><%= itinerary.location %></td>
    <td><%= itinerary.travel_on %></td>
    <td><%= link_to "Show", action: 'show', id: itinerary.id %>
  <tr>
<% end %>
</table>