<h1>Trip <%= @trip.id %></h1>
<ul class="list-unstyled">
  <li>Name: <%= @trip.name %></li>
</ul>
<%= link_to 'View Itineraries', trip_itineraries_path(@trip.id), class: 'btn btn-primary' %>
<%= form_for(@trip, method: :delete, html: {role: 'form'}) do |f| %>
	<%= f.submit 'Delete Trip', class: 'btn btn-primary' %>
	<%= link_to 'Back', action: 'index' %>
<% end %>